{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/prettier"
  ],
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": [
          "src"
        ]
      }
    },
    "react": {
      "version": "999.999.999"
    }
  },
  "rules": {
    "semi": 1,
    "no-console":0,
    "import/no-dynamic-require":0,
    "global-require":0
  }
}